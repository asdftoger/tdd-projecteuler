{
  "python.testing.pytestArgs": [
    "tests"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "my-code-actions.actions": {
    "[python]": {
      "create new methond {{diag:$1}}": {
        "diagnostics": ["\"(.*?)\" is not defined"],
        "text": "def {{diag:$1}}():\n    pass\n",
        "where": "afterLast",
      }
    }
  }
}